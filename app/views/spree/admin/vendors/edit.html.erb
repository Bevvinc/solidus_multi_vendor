<% admin_layout "full-width" %>

<% admin_breadcrumb(link_to plural_resource_name(Spree::Vendor), spree.admin_vendors_path) %>
<% admin_breadcrumb(@vendor.name) %>

<%= render "spree/shared/error_messages", target: @vendor %>

<%= form_for [:admin, @vendor] do |f| %>
  <fieldset>
    <%= render partial: "form", locals: { f: f } %>
    <%= render partial: "spree/admin/shared/edit_resource_links" %>
  </fieldset>
<% end %>
